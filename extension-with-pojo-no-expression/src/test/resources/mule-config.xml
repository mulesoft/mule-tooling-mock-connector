<?xml version="1.0" encoding="UTF-8"?>

<mule xmlns:pojo-no-expression="http://www.mulesoft.org/schema/mule/pojo-no-expression"
      xmlns="http://www.mulesoft.org/schema/mule/core" xmlns:doc="http://www.mulesoft.org/schema/mule/documentation"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://www.mulesoft.org/schema/mule/core http://www.mulesoft.org/schema/mule/core/current/mule.xsd
http://www.mulesoft.org/schema/mule/pojo-no-expression http://www.mulesoft.org/schema/mule/pojo-no-expression/current/mule-pojo-no-expression.xsd">
    <pojo-no-expression:ingredient name="Ingredient" amount="11" type="11" doc:name="Ingredient"/>
    <pojo-no-expression:complex-pojo name="Complex_Pojo" intField="3" doc:name="Complex Pojo"
                                     ingredientNoExpression="Ingredient">
        <pojo-no-expression:ingredient amount="10" type="10"/>
    </pojo-no-expression:complex-pojo>
    <pojo-no-expression:config name="Pojo_Config" doc:name="Pojo Config">
        <pojo-no-expression:connection complexPojoAtProvider="Complex_Pojo1"/>
    </pojo-no-expression:config>
    <pojo-no-expression:complex-pojo name="Complex_Pojo1" intField="90" doc:name="Complex Pojo"
                                     ingredientNoExpression="Ingredient">
        <pojo-no-expression:ingredient amount="10" type="10"/>
    </pojo-no-expression:complex-pojo>

    <pojo-no-expression:config name="Pojo_Config1" doc:name="Pojo Config">
        <pojo-no-expression:connection ingredientAtProvider="Ingredient">
        </pojo-no-expression:connection>
    </pojo-no-expression:config>
    <flow name="dummy-flow">
        <pojo-no-expression:operation-with-pojo config-ref="Pojo_Config1" doc:name="Pojo">
            <pojo-no-expression:complex-pojo-at-operation intField="9" ingredientNoExpression="Ingredient">
                <pojo-no-expression:ingredient amount="15" type="15"/>
            </pojo-no-expression:complex-pojo-at-operation>
            <pojo-no-expression:ingredient-at-operation amount="48" type="49"/>
        </pojo-no-expression:operation-with-pojo>
        <pojo-no-expression:operation-with-pojo config-ref="Pojo_Config" doc:name="Pojo">
            <pojo-no-expression:complex-pojo-at-operation intField="33" ingredientNoExpression="Ingredient" ingredient="Ingredient"/>
            <pojo-no-expression:ingredient-at-operation amount="22" type="22"/>
        </pojo-no-expression:operation-with-pojo>
        <pojo-no-expression:operation-with-pojo config-ref="Pojo_Config" complexPojoAtOperation="Complex_Pojo"
                                                doc:name="Pojo">
            <pojo-no-expression:ingredient-at-operation amount="22" type="22"/>
        </pojo-no-expression:operation-with-pojo>
    </flow>
</mule>
