taType"><xsd:annotation><xsd:documentation>Compensation Grade Data</xsd:documentation></xsd:annotation></xsd:element></xsd:sequence><xsd:attribute name="Add_Only" type="xsd:boolean"><xsd:annotation><xsd:documentation>Add Only</xsd:documentation></xsd:annotation></xsd:attribute><xsd:attribute ref="wd:version"/></xsd:complexType><xsd:complexType name="Put_Compensation_Grade_ResponseType"><xsd:annotation><xsd:documentation>Responds with the reference ID for the compensation grade.</xsd:documentation></xsd:annotation><xsd:sequence><xsd:element name="Compensation_Grade_Reference" type="wd:Compensation_GradeObjectType" minOccurs="0"><xsd:annotation><xsd:documentation>Compensation Grade Reference</xsd:documentation></xsd:annotation></xsd:element></xsd:sequence><xsd:attribute ref="wd:version"/></xsd:complexType><xsd:complexType name="Compensation_Plan_Request_ReferencesType"><xsd:annotation><xsd:documentation>Use this element to request specific Compensation Plans given the reference ID values</xsd:documentation></xsd:annotation><xsd:sequence><xsd:element name="Compensation_Plan_Reference" type="wd:Compensation_Assignable_PlanObjectType" maxOccurs="unbounded"><xsd:annotation><xsd:documentation>Compensation Plan Reference</xsd:documentation></xsd:annotation></xsd:element></xsd:sequence></xsd:complexType><xsd:complexType name="Compensation_Plan_Request_CriteriaType"><xsd:annotation><xsd:documentation>Values in this element can be used to filter the types of Compensation Plans returned</xsd:documentation></xsd:annotation><xsd:sequence><xsd:element name="Plan_Type_Reference" type="wd:Compensation_Assignable_Component_TypeObjectType" minOccurs="0" maxOccurs="unbounded"><xsd:annotation><xsd:documentation>Plan Type Reference elements are used to limit the compensation plans returned by a type.  The ID identifier attribute is "Compensation_Plan_Type_ID".  The supported type values are BONUS, FUTURE.</xsd:documentation></xsd:annotation></xsd:element><xsd:element name="Include_Inactive" type="xsd:boolean" minOccurs="0" maxOccurs="1"><xsd:annotation><xsd:documentation>Include Inactive indicates the response will also include inactive Compensation Plans and Guideline components.</xsd:documentation></xsd:annotation></xsd:element></xsd:sequence></xsd:complexType><xsd:complexType name="Compensation_Plan_Response_GroupType"><xsd:annotation><xsd:documentation>Use to limit the returned data for a given Compensation Plan</xsd:documentation></xsd:annotation><xsd:sequence><xsd:element name="Include_Reference" type="xsd:boolean" minOccurs="0" maxOccurs="1"><xsd:annotation><xsd:documentation>Include Reference</xsd:documentation></xsd:annotation></xsd:element></xsd:sequence></xsd:complexType><xsd:complexType name="Get_Compensation_Plans_RequestType"><xsd:annotation><xsd:documentation>Request element to get compensation plan information.  If no criteria is specified in the compensation plan request references or compensaiton plan request criteria elements, all compensation plans will be returned.</xsd:documentation></xsd:annotation><xsd:sequence><xsd:choice><xsd:element name="Request_References" type="wd:Compensation_Plan_Request_ReferencesType" minOccurs="0"><xsd:annotation><xsd:documentation>Request References</xsd:documentation></xsd:annotation></xsd:element><xsd:element name="Request_Criteria" type="wd:Compensation_Plan_Request_CriteriaType" minOccurs="0"><xsd:annotation><xsd:documentation>Request Criteria</xsd:documentation></xsd:annotation></xsd:element></xsd:choice><xsd:element name="Response_Filter" type="wd:Response_FilterType" minOccurs="0"><xsd:annotation><xsd:documentation>Response Filter</xsd:documentation></xsd:annotation></xsd:element><xsd:element name="Response_Group" type="wd:Compensation_Plan_Response_GroupType" minOccurs="0"><xsd:annotation><xsd:documentation>Response Group</xsd:documentation></xsd:annotation></xsd:element></xsd:sequence><xsd:attribute ref="wd:version"/></xsd:complexType><xsd:complexType name="Compensation_PlanType"><xsd:an