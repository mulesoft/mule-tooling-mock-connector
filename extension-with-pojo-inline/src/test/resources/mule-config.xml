    <flow name="dummy-flow">
        <pojo-inline:operation-with-pojo-inline config-ref="Pojo_Config" doc:name="Pojo">
            <pojo-inline:complex-pojo-at-operation intField="9">
                <pojo-inline:ingredient amount="15" type="15"/>
                <pojo-inline:ingredient-no-expression amount="15" type="15"/>
            </pojo-inline:complex-pojo-at-operation>
            <pojo-inline:ingredient-at-operation amount="48" type="49"/>
        </pojo-inline:operation-with-pojo-inline>
        <pojo-inline:operation-with-pojo-inline config-ref="Pojo_Config" doc:name="Pojo">
            <pojo-inline:complex-pojo-at-operation intField="33">
                <pojo-inline:ingredient amount="15" type="15"/>
                <pojo-inline:ingredient-no-expression amount="15" type="15"/>
            </pojo-inline:complex-pojo-at-operation>
            <pojo-inline:ingredient-at-operation amount="22" type="22"/>
        </pojo-inline:operation-with-pojo-inline>
    </flow>
</mule>
